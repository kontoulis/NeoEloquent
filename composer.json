{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": ["neo4j", "ogm", "graph", "database", "neoeloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "vinelab/neoeloquent-support": "5.*",
        "vinelab/neoeloquent-pagination": "5.*",
        "vinelab/neoeloquent-contracts": "5.2.*",
        "vinelab/neoeloquent-container": "5.*",
        "vinelab/neoeloquent-events": "5.*",
        "illuminate/database": "5.*",
        "vinelab/neoclient": "~3.4",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*",
        "symfony/var-dumper": "*"
    },
    "repositories": [

    ],
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
